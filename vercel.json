{
    "version": 2,
    "rewrites": [
        {
            "source": "/admin/:path*",
            "destination": "/admin/index.html"
        },
        {
            "source": "/(?!api|admin)(.*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}